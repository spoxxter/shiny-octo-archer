<h1>Listing job_results</h1>

<table>
  <thead>
    <tr>
      <th>Jobtitle</th>
      <th>Company</th>
      <th>Snippet</th>
      <th>Url</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @job_results.each do |job_result| %>
      <tr>
        <td><%= job_result.jobtitle %></td>
        <td><%= job_result.company %></td>
        <td><%= job_result.snippet %></td>
        <td><%= job_result.url %></td>
        <td><%= job_result.latitude %></td>
        <td><%= job_result.longitude %></td>
        <td><%= link_to 'Show', job_result %></td>
        <td><%= link_to 'Edit', edit_job_result_path(job_result) %></td>
        <td><%= link_to 'Destroy', job_result, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job result', new_job_result_path %>
